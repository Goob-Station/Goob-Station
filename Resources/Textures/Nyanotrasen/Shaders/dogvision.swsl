uniform sampler2D SCREEN_TEXTURE;

void fragment() {
    highp vec4 color = zTextureSpec(SCREEN_TEXTURE, UV);

    highp mat3 m = mat3(
        vec3(0.625,0.375,0.000),
        vec3(0.700,0.300,0.000),
        vec3(0.000,0.300,0.700)
    );
    highp vec3 result = color.rgb * m;

    COLOR = vec4(result, 1);
}
